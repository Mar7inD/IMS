<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.RemoveCountryController">
   <children>
      <HBox alignment="CENTER" layoutX="186.0" layoutY="326.0" prefHeight="67.0" prefWidth="203.0">
         <children>
            <Button fx:id="cancel" mnemonicParsing="false" onAction="#onClick" text="Cancel">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="remove" mnemonicParsing="false" onAction="#onClick" text="Remove">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
      <ComboBox fx:id="options" layoutX="213.0" layoutY="130.0" prefWidth="150.0" />
      <Label layoutX="95.0" layoutY="59.0" text="Select country from the combo box and then remove it.">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
